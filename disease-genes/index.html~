<script type="text/javascript">

$( 'body' ).on( 'mousedown', '#table_select_button .btn', function(event) {
    event.stopPropagation(); // prevent default bootstrap behavior
    console.log(this)
    if( $(this).attr('data-toggle') != 'button' ) { // don't toggle if data-toggle="button"
        //$(this).toggleClass('active');
        $(this).button('toggle')
    }
    //alert( $(this).hasClass( 'active' ) ); // button state AFTER the click
    var myRadio = $('input[name=selected_table]');
    var checkedValue = myRadio.filter(':checked').val();
    //console.log(checkedValue)
    $( "#dynamic_visible" ).html('<div id="dynamic_visible">Generating Table...</div>');
    dynamicTableFromURL('visible', 'data/{0}-summary-table.txt'.f(checkedValue), aoColumnDefs);
});

</script>


