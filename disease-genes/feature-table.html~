<script type="text/javascript" charset="utf-8" src="js/jquery-2.1.0.js"></script>
<script src="js/bootstrap.min.js"></script>
<script type="text/javascript" charset="utf-8" src="js/jquery.dataTables.min.js"></script>
<script type="text/javascript" charset="utf-8" src="js/jquery.csv-0.71.min.js"></script>
<script type="text/javascript" charset="utf-8" src="js/tablefxns.js"></script>

<script type='text/javascript'>
    var url_vars = getUrlVars()
    var feature = url_vars['feature']


    var info_path = 'data/feature-info/{0}.json'.f(feature);
    if (! fso.FolderExists(info_path)) {window.location = './';}
    jQuery.getJSON(info_path, success=function( json ) {
        console.log(json);
		html_heading = '<h2>Performance of the <i>{0}</i> Feature'.f(json['easy_name'])
        jQuery('#heading').html(html_heading);

		var html_metapath = json['metapath_long']
		html_metapath = html_metapath.split('-').join(') <small> ');
		html_metapath = html_metapath.split('~').join(' </small> (');
		html_metapath = '({0})'.f(html_metapath)



        var html_list = [];
        html_list.push('<dl class="dl-horizontal">');

        html_list.push('<dt>Metric</dt><dd>{0}</dd>'.format(json['metric']));
        html_list.push('<dt>Metapath</dt><dd>{0}</dd>'.format(html_metapath));
        html_list.push('<dt>Description</dt><dd>{0}</dd>'.format(json['description']));

        html_list.push('</dl>');
        jQuery('#feature_info').html(html_list.join(''));
    });



    var file_path = 'data/feature-tables/{0}.txt'.f(feature)
    dynamicTableFromURL('feature', file_path, aoColumnDefs)
</script>

